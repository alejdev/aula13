{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "a13",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "a13",
      "kebab-case"
    ],
    // Mine
    "semicolon": [true, "never"],
    "eofline": true,
    "max-line-length": [
      false
    ]
  }
}
